<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema targetNamespace="http://www.superwebservice.com/fun" 
	xmlns:tns="http://www.superwebservice.com/fun" 
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	elementFormDefault="qualified">

	<xsd:element name="situation-meteo" >
		<xsd:complexType>
			<xsd:sequence>

				<xsd:element name="GetWeather">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element minOccurs="0" maxOccurs="1" name="CityName" type="tns:string" />
							<xsd:element minOccurs="0" maxOccurs="1" name="CountryName" type="tns:string" />
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>

				<!--  On a un complextype "produitS" composÃ© de plusieurs "produit" -->
				<xsd:element name="reponse-meteo">
					<xsd:complexType>
						<xsd:element name="meteo">
							<xsd:complexType>
								<xsd:sequence>							             
									<xsd:element name="lieu" type="xsd:string" />   
									<xsd:element name="date-heure" type="xsd:string" />
									<xsd:element name="vent" type="xsd:string" />
									<xsd:element name="visibilite" type="xsd:string" />
									<xsd:element name="ciel" type="xsd:string" />
									<xsd:element name="temperature" type="xsd:string" /> 
									<xsd:element name="point-rosee" type="xsd:string" />
									<xsd:element name="humidite" type="xsd:string" />
									<xsd:element name="pression" type="xsd:string" />
									<xsd:element name="status" type="xsd:string" />        						             
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:complexType>
				</xsd:element>

			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
